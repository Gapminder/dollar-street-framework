{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2015",
    "outDir": "../dist/server-cron",
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "allowJs": true,
    "declaration": false,
    "moduleResolution": "node",
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "include": [
    "src/cron.task.ts",
    "src/app/crontasks/currency.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
